var Upgrade={getSmsCredits:function(){var n=$("#"+ClientID_Upgrade.ddlSmsRecharge).val();n!=""?$("#"+ClientID_Upgrade.lblSmsCredits).text(formatNumber(n)+" SMS"):$("#"+ClientID_Upgrade.lblSmsCredits).text("0")},getPortfolioAmount:function(){var n=$("#"+ClientID_Upgrade.ddlPortfolioPeriod).find("option:selected").val(),t;n!=""?(t=parseFloat(n).toFixed(2),$("#"+ClientID_Upgrade.lblPortfolioAmount).text("Rs. "+formatAmount(t))):$("#"+ClientID_Upgrade.lblPortfolioAmount).text("0")},validateSmsRecharge:function(n){var i,r,t;return $("#"+ClientID_Upgrade.divErrorSms).hide(),t=$(n).attr("onclick"),$(n).attr("onclick",""),i="",r=!0,validateDropdownById(ClientID_Upgrade.ddlSmsRecharge,"")||(i+="<li>SMS Recharge is required<\/li>"),i!=""?(showValidationError(ClientID_Upgrade.divErrorSms,"Could not submit your SMS recharge request due to following errors:","<ul>"+i+"<\/ul>"),r=!1,t=$(n).attr("onclick",t)):showProcessing(),r},validatePortfolioRenew:function(n){var i,r,t;return $("#"+ClientID_Upgrade.divErrorPortfolio).hide(),t=$(n).attr("onclick"),$(n).attr("onclick",""),i="",r=!0,validateDropdownById(ClientID_Upgrade.ddlPortfolioPeriod,"")||(i+="<li>Portfolio Period is required<\/li>"),i!=""?(showValidationError(ClientID_Upgrade.divErrorPortfolio,"Could not submit your Portfolio Tracker renew request due to following errors:","<ul>"+i+"<\/ul>"),r=!1,t=$(n).attr("onclick",t)):showProcessing(),r}},Tour={showPopover:function(n){var t="",i="",r="",u,f=0;switch(n){case"ADD_PORTFOLIO":t=ClientID_Portfolio.lnkAddPortfolio;i="Add Portfolio";r="Click here to add a new portfolio"+Tour.getNavigation("","","VIEW_PORTFOLIO","");u="right";break;case"VIEW_PORTFOLIO":t=ClientID_Portfolio.lbtnSearch;i="Search Portfolio";r="Click here to search for created portfolio"+Tour.getNavigation("ADD_PORTFOLIO","","ADD_SHAREHOLDER","Shareholder.aspx");u="right";break;case"ADD_SHAREHOLDER":t=ClientID_Shareholder.lnkAdd;i="Add Shareholder";r="Click here to add a new shareholder"+Tour.getNavigation("ADD_PORTFOLIO","Portfolio.aspx","ADD_SHARE","MyPortfolio.aspx");u="right";break;case"ADD_SHARE":t=ClientID_MyPortfolio.lnkAddShare;i="Add Purchased Shares";r="Click here to add purchased shares"+Tour.getNavigation("ADD_SHAREHOLDER","Shareholder.aspx","SELL_SHARE","");u="right";f=340;break;case"SELL_SHARE":t=ClientID_MyPortfolio.lnkSellShare;i="Add Sold Shares";r="Click here to add sold shares"+Tour.getNavigation("ADD_SHARE","","ADD_WATCHLIST","MyWatchlist.aspx");u="right";f=340;break;case"ADD_WATCHLIST":t=ClientID_MyWatchlist.lnkAddWatchlist;i="Add Watchlist";r="Click here to add a company to your watchlist"+Tour.getNavigation("SELL_SHARE","MyPortfolio.aspx","SHOWALL_WATCHLIST","");u="left";break;case"SHOWALL_WATCHLIST":t=ClientID_MyWatchlist.lnkAll;i="Show Companies";r="Click here to show <strong class='text-success'>advancing<\/strong>, <strong class='text-danger'>declining<\/strong>, <strong class='text-warning'>no-change<\/strong> or all companies"+Tour.getNavigation("ADD_WATCHLIST","","VIEW_PROFITLOSS","ProfitLossReport.aspx");u="right";break;case"VIEW_PROFITLOSS":t=ClientID_ProfitLoss.lbtnSearchReport;i="Search Report";r="Click here to view profit and loss report"+Tour.getNavigation("SHOWALL_WATCHLIST","MyWatchlist.aspx","START_TOUR","");u="right";f=340;break;case"START_TOUR":t="lnkStartTour";i="Start Tour";r="Click here if you want to take this tour again"+Tour.getNavigation("VIEW_PROFITLOSS","","","");u="bottom"}$("#"+t).attr("title","");$("a").popover("destroy");$("#"+t).popover({html:!0,placement:u,content:r,title:i,trigger:"manual"});$("body").has(".popover-overlay").length==0&&$("body").append('<div class="popover-overlay"><\/div>');$("#"+t).popover("show");f>0?$("html, body").animate({scrollTop:$("#"+t).offset().top-f},"slow"):$("html, body").animate({scrollTop:0},"slow");hideProcessing()},getNavigation:function(n,t,i,r){return"<div class='popover-footer clearfix'>"+Tour.getPrevButton(n,t)+" "+Tour.getNextButton(i,r)+" "+Tour.getEndTourButton()+"<\/div>"},getPrevButton:function(n,t){return n==""?"":t!=""?"<a class='btn btn-primary btn-xs' onclick='showProcessing();' href='/"+t+"?tcode="+n+"'><span class='icon-chevron-left'><\/span> Prev<\/a>":"<a class='btn btn-primary btn-xs' href='javascript:void(0);' onclick='showProcessing();Tour.showPopover(\""+n+"\");'><span class='icon-chevron-left'><\/span> Prev<\/a>"},getNextButton:function(n,t){return n==""?"":t!=""?"<a class='btn btn-primary btn-xs' onclick='showProcessing();' href='/"+t+"?tcode="+n+"'>Next <span class='icon-chevron-right'><\/span><\/a>":"<a class='btn btn-primary btn-xs' href='javascript:void(0);' onclick='showProcessing();Tour.showPopover(\""+n+"\");'>Next <span class='icon-chevron-right'><\/span><\/a>"},getEndTourButton:function(){return"<a class='btn btn-danger btn-xs pull-right' href='/MyPortfolio.aspx' onclick='showProcessing();'>End Tour<\/a>"}}